    <Window.Resources>
        <CollectionViewSource x:Key="pagosViewSource" d:DesignSource="{d:DesignInstance my:Pagos, CreateList=True}" />
    </Window.Resources>
    <Grid DataContext="{StaticResource pagosViewSource}">
        <Label Content="Nro. de Cedula" Height="28" HorizontalAlignment="Left" Margin="12,83,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Formulario para ingreso de cuotas" Height="33" HorizontalAlignment="Center" Margin="86,12,86,0" Name="label2" VerticalAlignment="Top" Width="300" FontFamily="Calibri" FontSize="20" FontWeight="Bold" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="107,85,0,0" Name="textBox1" VerticalAlignment="Top" Width="89" KeyDown="textBox1_KeyDown" />
    </Grid>
</Window>
